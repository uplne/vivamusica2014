<ul class="news" data-module="carousel">
    {{#foreach news}}
    <li class="pull--left">
        <a href="/program/1" class="news__link">
            <span class="title--alpha">{{title1}}</span>
            <span class="title--beta">{{title2}}</span>
            <img class="box__m" src="{{img}}" alt="" />
        </a>
    </li>
    {{/foreach}}
</ul>
<a class="arrow--prev" data-move="prev" href="#nogo">Prev</a>
<a class="arrow--next" data-move="next" href="#nogo">Next</a>
